{
  "name": "budget-tracker",
  "version": "1.0.0",
  "description": "DHL-themed budget tracker & asset list",
  "scripts": {
    "dev": "tsx --env-file .env src/server.ts",
    "postinstall": "node -e \"require('fs').copyFileSync('./node_modules/@azure/msal-browser/lib/msal-browser.min.js', './public/msal-browser.min.js')\"",
    "db:migrate": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:reset": "prisma migrate reset --force"
  },
  "dependencies": {
    "@azure/msal-browser": "^5.2.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/static": "^6.12.0",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^4.0.1",
    "@prisma/client": "^5.22.0",
    "fastify": "^4.28.1",
    "jsonwebtoken": "^9.0.3",
    "jwks-rsa": "^3.2.2",
    "pino-pretty": "^13.0.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.13.1",
    "prisma": "^5.22.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
